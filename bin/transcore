#!/usr/bin/env ruby

require "transcore"

command = ARGV.shift

exit_code = 0

case command
when "transpose"
  exit_code = Transcore::Command::Transpose.run(ARGV) || 0
when "convert"
  exit_code = Transcore::Command::Convert.run(ARGV) || 0
else
  STDERR.puts "Invalid command: '#{command}'"
  exit_code = 1
end

exit exit_code
